#include <iostream>
#include <vector>

// insertion sort
void insertion_sort(std::vector<int> & arr){
    int temp;
    int n = sizeof(arr) / sizeof(int);
    for(int i = 1; i < n; i++){
        for(int j = 0; j < i; j++){
            if(arr[i] <= arr[j]){
                temp = arr[j];
                arr[j] = arr[i];
                arr[j + 1] = temp;
            }
        }
    }
}

