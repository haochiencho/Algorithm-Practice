bool isLeapYear(int n){
    if(n == 100)
        return true;
    else if(n % 100 == 0)
        return false;
    else if(n % 4 == 0)
        return true;
    else
        return false;
}

int main(){
    FILE *fin, *fout;
    fin = fopen("friday.in", "r");
    fout = fopen("friday.out", "w");
    
    int n = 0;
    fscanf(fin, "%d", &n);
    vector<int> days(7, 0);
    int curDay = 0;
    int month[] = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
    for(int i = 0; i < n; i++){
        for(int j = 0; j < 12; j++){
            days[(curDay + 13 - 1) % 7]++;
            curDay += month[j];
            if(j == 1 && isLeapYear(i))
                curDay++;
        }
    }
    for(int i = 0; i < days.size(); i++){
        if(i == 6)
            fprintf(fout, "%d", days[(5 + i) % 7]);
        else
            fprintf(fout, "%d ", days[(5 + i) % 7]);
        
    }
    fprintf(fout, "\n");
}
